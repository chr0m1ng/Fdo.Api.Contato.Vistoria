{
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "Settings": {
    "ApiVersion": "v1",
    "StoragePath": "D:/Contato",
    "ApiKey": "default",
    "AuthHeader": "x-api-key"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "System": "Information",
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "https://www.google.com"
        }
      }
    ],
    "Enrich": [ "FromLogContext" ]
  }
}
